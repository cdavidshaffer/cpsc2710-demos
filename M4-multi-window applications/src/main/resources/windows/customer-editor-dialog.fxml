<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.au.cpsc.multiwindow.windowcontroller.CustomerEditorDialogController">
   <center>
      <fx:include fx:id="customerDetailEditor" source="../views/customer-detail-editor.fxml" />
   </center>
   <bottom>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
            <Button cancelButton="true" mnemonicParsing="false" onAction="#cancelButtonAction" text="Cancel" />
          <Button defaultButton="true" mnemonicParsing="false" onAction="#saveButtonAction" text="Save" />
        </buttons>
         <BorderPane.margin>
            <Insets left="5.0" right="5.0" />
         </BorderPane.margin>
      </ButtonBar>
   </bottom>
</BorderPane>
