<VBox alignment="CENTER" spacing="20.0" stylesheets="@style/main.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csspropertiesdemo.CssPropertiesDemoController">
