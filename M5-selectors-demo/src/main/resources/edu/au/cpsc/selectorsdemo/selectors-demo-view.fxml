<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>

<VBox alignment="CENTER" prefHeight="655.0" prefWidth="590.0" spacing="20.0" stylesheets="@style/main.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.au.cpsc.selectorsdemo.SelectorsDemoController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>

  <Label text="First label" />
   <Label text="Second label">
      <styleClass>
         <String fx:value="large" />
         <String fx:value="bordered" />
      </styleClass></Label>
  <Button text="First button" />
   <Button id="second-button" mnemonicParsing="false" text="Second button" />
   <Button cancelButton="true" mnemonicParsing="false" styleClass="large" text="Third button" />
   <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="200.0" stroke="BLACK" strokeType="INSIDE" width="200.0" />
   <HBox id="bottom-row" prefHeight="100.0" prefWidth="200.0" spacing="5.0">
      <children>
         <TextField text="A TextField" />
         <Label text="A label in a HBox" />
         <ComboBox prefWidth="150.0" promptText="A combobox" />
         <Label text="Another lab in an HBox" />
         <VBox prefHeight="200.0" prefWidth="100.0">
            <children>
               <Label text="Label" />
               <Label text="Label" />
               <Label text="Label" />
            </children>
         </VBox>
      </children>
   </HBox>
</VBox>
