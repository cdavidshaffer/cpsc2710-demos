    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  <children>
    <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0">
      <children>
        <Rectangle arcHeight="5.0" arcWidth="5.0" height="200.0" stroke="BLACK" strokeType="INSIDE" width="200.0" />
            <Group>
               <children>
              <Circle id="left-circle" radius="64.0" stroke="BLACK" strokeType="INSIDE" />
                  <Circle id="right-circle" layoutX="54.0" layoutY="-43.0" radius="64.0" stroke="BLACK" strokeType="INSIDE" />
               </children>
            </Group>
      </children>
    </HBox>
      <VBox>
         <children>
            <Button mnemonicParsing="false" styleClass="btn-copy" text="Copy" />
            <Button mnemonicParsing="false" styleClass="btn-cut" text="Cut" />
            <Button mnemonicParsing="false" styleClass="btn-paste" text="Paste" />
